<div style="display:none;">
  <div id="hello" class="fancybox-window popup-margin">
  	<%= form_for @popup_contact_form do |f| %>
		<div class = "form-group paddings-2">
				<label for="InputName"></label>
				<%= f.text_field :name, class: "form-control PopUpInput", placeholder: t('callback.name') %>
			</div>
			<div class = "form-group">
				<label for="InputName"></label>
				<%= f.text_field :phone, class: "form-control PopUpInput", placeholder: t('callback.phone') %>	
			</div>
			<div class="PopUpInput center-block">
				<%= button_tag(type: 'submit', class: "btn btn-success button-contact-form button-get-price", 
				id: "get-price") do %> 
				<%= t('callback.button') %>
				<% end %>
			</div>
			<!-- <%= render 'popup_contact_form/test' %> -->
	<% end %>
  </div>
</div>