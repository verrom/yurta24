
<h2 class="center-block title-1 title-1-about-us " style="float: none" id="photos">
			<%= t('photogallery.title') %></h2>
<div class="container" style="float: none">
	<div class="row" style="float: none">
		<% @pictures.each do |picture| %>
			<div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 photogallery">
					<%= link_to image_tag(picture.image.url, :size => "260x180"),
							picture.image.url,
					class: "grouped_elements", rel: "gallery1" %>
			</div>
		<% end %>
	</div>
</div>